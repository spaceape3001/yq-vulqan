    int         encode(KeyCode kc, KeyCodex cdx)
    {
        switch(cdx){
        case KeyCodex::GLFW:
            return encode_glfw(kc);
        case KeyCodex::ImGui:
            return encode_imgui(kc);
        default:
            return -1;
        }
    }
    
